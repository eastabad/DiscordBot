# Discord配置 (必需)
DISCORD_TOKEN=your_discord_bot_token_here

# TradingView图表API配置 (可选，但推荐)
CHART_IMG_API_KEY=your_chart_img_api_key
LAYOUT_ID=your_layout_id
TRADINGVIEW_SESSION_ID=your_tradingview_session_id
TRADINGVIEW_SESSION_ID_SIGN=your_tradingview_session_sign

# Discord频道监控配置 (可选)
MONITOR_CHANNEL_IDS=1234567890,0987654321

# Webhook配置 (可选)
WEBHOOK_URL=https://your-webhook-url.com/webhook

# VPS部署配置 (VPS用户无限制使用)
# 设置为true启用VPS部署模式，所有用户无使用次数限制
VPS_DEPLOYMENT=true